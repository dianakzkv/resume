<main>
  <div>
    {{#with page_header}}
      <h1 class='text-center'>{{title}}</h1>
      <p>
        <div
          class='text-center text-uppercase'
        >{{description}}</div>
      </p>
    {{/with}}

    {{#with section}}
      <section>
        <div class='card'>
          <div class='card-body'>
            {{#with about_us}}
              {{#with title_list}}
                <div
                  class='display-6 text-end'
                >{{main}}</div>
                <br />
                <h2 class='text-end'>{{sub}}</h2>
              {{/with}}
              {{#with button}}
                <button
                  class='btn btn-outline-info'
                >{{text}}</button>
              {{/with}}
            {{/with}}
          </div>
        </div>

        <br />

        <div class='card'>
          <div class='card-body'>
            {{#with advantage}}
              <div
                class='display-5 text-muted'
              >{{title}}</div>
              <br />
              <div class='card'>
                <div class='card-body'>
                  {{#with efficiency}}
                    <div
                      class='display-4 text-muted'
                    >{{title}}</div>
                    <br />
                    <h6>{{sub_title}}</h6>
                    <p>{{description}}</p>
                    <button
                      class='btn btn-outline-warning'
                    >{{button}}</button>
                  {{/with}}
                </div>
              </div>
              <br />
              <div class='card'>
                <div class='card-body'>
                  {{#with innovation}}
                    <div
                      class='display-4 text-muted text-center'
                    >{{title}}</div>
                    <br />
                    <h6
                      class='text-center'
                    >{{sub_title}}</h6>
                    <p
                      class='text-center'
                    >{{description}}</p>
                    <button
                      class='btn btn-outline-warning'
                    >{{button}}</button>
                  {{/with}}
                </div>
              </div>
              <br />
              <div class='card'>
                <div class='card-body'>
                  {{#with collaboration}}
                    <div
                      class='display-4 text-muted text-end'
                    >{{title}}</div>
                    <br />
                    <h6 class='text-end'>{{sub_title}}</h6>
                    <p class='text-end'>{{description}}</p>
                    <button
                      class='btn btn-outline-warning'
                    >{{button}}</button>
                  {{/with}}
                </div>
              </div>
            {{/with}}
          </div>
        </div>

    <br>

        <div class='card'>
            <div class='card-body'>
                {{#with agreement}}
                    <div
                        class='display-5'
                    >{{title}}</div>

                    <br />

                    <div class='card'>
                        <div class='card-body'>
                            {{#with info}}
                                <h5
                                class='display-4'
                                >{{sub_title}}</h5>

                                <br />
                                <div class='card'>
                                <div class='card-body'>
                                    {{#with detail}}
                                        <p>{{text}}</p>
                                        <button
                                        class='btn btn-success'
                                        >{{button}}</button>
                                    {{/with}}
                            {{/with}}
                        </div>
                    </div>
                {{/with}}
            </div>
        </div>

      </section>
    {{/with}}
  </div>
</main>